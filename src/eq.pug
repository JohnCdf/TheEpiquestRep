html#body(lang='en')
  head
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    meta(charset='utf-8')
    link(rel='shortcut icon' type='image/png' href='../static/EQF.png')
    each style in styles
      link(rel='stylesheet' href=style)
    each script in scripts
      script(src=script)
    title EPIQUEST
  body
    h1#mainh EPIQUEST
    br
    .bar
      .sound.off(title='Sound')
      #stats
        .name.icon(title='User Name')
        span#name
        .money.icon(title='Money')
        span#money
        .safe.icon(title='Safe')
        span#safe
        .rod.icon(title='Inventory')
        span#rod
        .level.icon(title='Level')
        span#level
        .xp.icon(title='XP')
        span#xp
        .armor.icon(title='Armor')
        span#armor
        .weapon.icon(title='Weapon')
        span#weapon
    br
    #controls
      h2#display . . .
      br
      form#input
        input#inputBox(type='text' placeholder='type here...')
        button#submit Submit
      .buttons
    a.github-corner(href='https://github.com/JohnCdf/TheEpiquestRep/' target='_blank')
      svg(
        width='80'
        height='80'
        viewBox='0 0 250 250'
        style='fill: #151513; color: #c19d64; position: absolute; top: 0; border: 0; right: 0;'
        aria-hidden='true'
      )
        path(d=pathes[0])
        path.octo-arm(d=pathes[1] fill='currentColor' style='transform-origin: 130px 106px;')
        path.octo-body(d=pathes[2] fill='currentColor')
